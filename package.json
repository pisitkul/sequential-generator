{
  "name": "sequential-generator",
  "version": "1.1.0",
  "description": "A simple generator for creating sequential codes with prefix and date",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/cjs/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/cjs/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "test": "jest",
    "test:cov": "jest --coverage",
    "build": "npm run clean && npm run build:cjs && npm run build:esm && npm run build:esm-pkg",
    "build:cjs": "tsc",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:esm-pkg": "node -e \"require('fs').writeFileSync('dist/esm/package.json', JSON.stringify({type:'module'}))\"",
    "clean": "node -e \"require('fs').rmSync('dist', {recursive:true, force:true})\"",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "three",
    "code",
    "prefix",
    "generator",
    "date",
    "invoice",
    "unique"
  ],
  "author": "shitdogg <pratchaya.ueapisitkul@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pisitkul/sequential-generator.git"
  },
  "license": "MIT",
  "type": "commonjs",
  "dependencies": {
    "dayjs": "^1.11.19"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^25.3.0",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
